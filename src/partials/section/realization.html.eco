<h2 class="section-title"><%= @labels['realization'] %></h2>

<ul class="sponsors-list sponsor-diamante">

<% for company in @realization: %>
  <li class="sponsor-item" itemscope itemtype="http://schema.org/Organization">
    <a href="<%= company.url %>?utm_source=frontincuiaba2015" class="sponsor-logo sponsor--link" itemprop="url">
      <img src="<%= company.logo %>" alt="<%= company.name %>" class="photo" itemprop="image">
    </a>
  </li>
<% end %>
</ul>